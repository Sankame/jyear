$(function(){
$('#menu .archive').click(function(e){
$('+ul.subMenu',this).slideToggle( 450
,  'linear'
,  changeMenuIcon( $('#menu .archive a'), ! $('.subMenu').isVisible() ) );
});
});

$(function(){
var isOpened = getCookie(cookieNameMediaMenuOpen);

var isOpenedFlag = (isOpened ? true : false);

if(isOpenedFlag){
$('.subMenu').show();
}else{
$('.subMenu').hide();
}
$('+ul.subMenu',this).slideToggle( 450
,  'linear'
,  changeMenuIcon($('#menu .archive a'), isOpenedFlag) );
});

function changeMenuIcon(element, isOpened){

if(isOpened){
element.html('<i class="icon-caret-down"></i> close');
}else{
element.html('<i class="icon-caret-right"></i> open');
}
setCookie(cookieNameMediaMenuOpen, isOpened);
}
