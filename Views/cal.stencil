function changeWikipediaIcon(element, isOpened){

if(isOpened){
element.html('<i class="icon-caret-down"></i> wikipedia close');
}else{
element.html('<i class="icon-caret-right"></i> wikipedia open');
}
setCookie(cookieWikipediaOpen, isOpened);
}
/** ----------------wikipedia関連の処理---------------- */

function lightOn(element){
element.style.backgroundColor = '#ffffab';
}

function lightOff(element){
element.style.backgroundColor = '#ffffff';
}
/** 検索キーワード削除 */
function removeKeyword(){
$("#search-by-keyword").val('');
}

/** 検索 */
function searchByKeyword(){
var queryString = '';
var keyword		= jQuery.trim( $("#search-by-keyword").val() );

if(keyword != ''){
queryString = '?w=' + keyword;
}

var siteList = getSiteNameFilter();
if(siteList != ''){
queryString = queryString + (queryString!='' ? '&' : '?') + 's=' + siteList;
}
//cl(queryString);
location.href = baseUrl + queryString;
}

/** サイトによる絞り込み */
function searchNews(){
/*
var siteNameExpression		  = "input:checkbox[name='site_name[]']";
var checkedSiteNameExpression = "input:checkbox[name='site_name[]']:checked";

var checked = [];

$(checkedSiteNameExpression).each(function(){
checked.push(this.value);
});
var queryString;

if(checked.length === 0){
queryString = '';
}else{
//全チェックの場合、QueryStringでサイトを指定しない。
if($(siteNameExpression).length === checked.length){
queryString = '';
}else{
queryString = '?s=' + checked.join(',');
}
}
*/

//location.href = baseUrl + queryString;
var siteList = getSiteNameFilter();
location.href = baseUrl + (siteList!='' ? '?s=' + siteList : '');
}

function getSiteNameFilter(){
var siteNameExpression		  = "input:checkbox[name='site_name[]']";
var checkedSiteNameExpression = "input:checkbox[name='site_name[]']:checked";

var checked = [];

$(checkedSiteNameExpression).each(function(){
checked.push(this.value);
});
var queryString;

if(checked.length === 0){
queryString = '';
}else{
//全チェックの場合、QueryStringでサイトを指定しない。
if($(siteNameExpression).length === checked.length){
queryString = '';
}else{
queryString = checked.join(',');
}
}
return queryString;
}

$(document).ready(function(){
readyColorbox();
});

function readyColorbox(){
//Examples of how to assign the ColorBox event to elements
$(".group1").colorbox({rel:'group1'});
$(".group2").colorbox({rel:'group2', transition:"fade"});
$(".group3").colorbox({rel:'group3', transition:"none", width:"75%", height:"75%"});
$(".group4").colorbox({rel:'group4', slideshow:true});
$(".ajax").colorbox();
$(".youtube").colorbox({iframe:true, innerWidth:425, innerHeight:344});
$(".vimeo").colorbox({iframe:true, innerWidth:500, innerHeight:409});
$(".iframe").colorbox({iframe:true, width:"75%", height:"100%"});

$(".inline").colorbox({inline:true, width:"50%"});
$(".callbacks").colorbox({
onOpen:function(){ alert('onOpen: colorbox is about to open'); },
onLoad:function(){ alert('onLoad: colorbox has started to load the targeted content'); },
onComplete:function(){ alert('onComplete: colorbox has displayed the loaded content'); },
onCleanup:function(){ alert('onCleanup: colorbox has begun the close process'); },
onClosed:function(){ alert('onClosed: colorbox has completely closed'); }
});

$('.non-retina').colorbox({rel:'group5', transition:'none'})
$('.retina').colorbox({rel:'group5', transition:'none', retinaImage:true, retinaUrl:true});

//Example of preserving a JavaScript event for inline calls.
$("#click").click(function(){
$('#click').css({"background-color":"#f00", "color":"#fff", "cursor":"inherit"}).text("Open this window again and this message will still be here.");
return false;
});
}

/** Cookieセット */
function setCookie(cookieName, isSet){
if(isSet){
$.cookie(cookieName, 1, {path:"/", expires:365});
}else{
$.cookie(cookieName, "", {path:"/", expires:-1});
}
}

/** Cookie取得 */
function getCookie(cookieName){
return $.cookie(cookieName);
}

