//チェックボックス一括チェックON/OFF
$(function(){
$('#check-all').click(function(){
if(this.className == 'icon-check-empty'){
$('#site-list input').attr('checked','checked');
this.className = 'icon-check';
}else{
$('#site-list input').removeAttr('checked');
this.className = 'icon-check-empty';
}
});
});

$(function() {
var topBtn = $('#page-top');
topBtn.hide();
//スクロールが100に達したらボタン表示
$(window).scroll(function () {
if ($(this).scrollTop() > 100) {
topBtn.fadeIn();
} else {
topBtn.fadeOut();
}
});
//スクロールしてトップ
topBtn.click(function () {
$('body,html').animate({
scrollTop: 0
}, 500);
return false;
});
});


</script>

<script language="javascript" type="text/javascript">

$(function() {

