<!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
<link rel="shortcut icon" href="favicon.ico">

<link href='https://fonts.googleapis.com/css?family=Roboto:400,300,600,400italic,700' rel='stylesheet' type='text/css'>
<link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>

<!-- Animate.css -->
<link rel="stylesheet" href="/simple-cal/css/animate.css">
<!-- Icomoon Icon Fonts-->
<link rel="stylesheet" href="/simple-cal/css/icomoon.css">
<!-- Bootstrap  -->
<link rel="stylesheet" href="/simple-cal/css/bootstrap.css">
<!-- Owl Carousel -->
<link rel="stylesheet" href="/simple-cal/css/owl.carousel.min.css">
<link rel="stylesheet" href="/simple-cal/css/owl.theme.default.min.css">
<!-- Theme style  -->
<link rel="stylesheet" href="/simple-cal/css/style.css">

<style>
table, th{
    text-align: center;
}
table.cal{
    border:solid 1px grey;
    width:100%;
    height:250px;
color:#000000;
}
table.cal th{
    border:solid 1px grey;
}
td.date{
    border:solid 1px grey;
}
td.saturday{
    color:#1D39C0;
    background-color:#EDF8FE;
}
td.sunday{
    color:#FF0000;
    background-color:#FFDDDD;
}
td.holiday{
    color:#FF0000;
    background-color:#FFDDDD;
}
/* 土日祝日より後に定義する */
td.today{
    background-color:#FFFF80;
}
.work-item{
    /* margin-bottom: 100px; */
}
.element-center{
    text-align:center;
}
div.year-label{
    display:inline-block;
    width:40%;
}
h3.month-label{
    display:inline-block;
    margin-bottom:10px;
}
</style>


</head>
<body>
<div id="fh5co-page">
<a href="#" class="js-fh5co-nav-toggle fh5co-nav-toggle"><i></i></a>
<aside id="fh5co-aside" role="complementary" class="border js-fullheight">

<h1 id="fh5co-logo">
<a href="{{baseUrl}}">
<img src="/simple-cal/images/cal-logo-8.png" alt="Free HTML5 Bootstrap Website Template">
</a>
</h1>
<nav id="fh5co-main-menu" role="navigation">
<ul>
<li><h3 class="fh5co-heading-colored fh2co-heading">{{appName}}</h3></li>
<li class="fh5co-active"><a href="{{baseUrl}}">Home</a></li>
</ul>
</nav>

<div class="fh5co-footer">
<p>
<small>
<span>&copy; 2017 {{appName}} All Rights Reserved.</small></span>
<span>Template Design <a href="http://freehtml5.co/" target="_blank">FreeHTML5.co</a> </span>
</small>
</p>
</div><!-- fh5co-footer -->

</aside>

<div id="fh5co-main">

<div class="fh5co-narrow-content">
<h2 class="fh5co-heading-colored fh5co-heading animate-box element-center" data-animate-effect="fadeInLeft">{{targetYear}}年</h2>
<div class="row animate-box" data-animate-effect="fadeInLeft">

<div class="col-md-4 col-sm-6 col-xs-6 col-xxs-12 work-item">
{% include "month1.stencil" %}
</div>

<div class="col-md-4 col-sm-6 col-xs-6 col-xxs-12 work-item">
